  "scripts": {
    "start": "NODE_ENV=development node server/index.mjs",
    "debug": "NODE_ENV=development node --inspect --preserve-symlinks server/index.mjs",
    "vendor": "NODE_ENV=development node ./scripts/vendor.mjs",
    "lint": "eslint --ext .ts,.js,.tsx client/",
    "test-production": "NODE_ENV=production node server/index.mjs --test --s3proxy --nocache --pretty",
    "run-production": "NODE_ENV=production node server/index.mjs --s3proxy",
    "watch": "NODE_ENV=development npx nodemon --watch server server/index.mjs",
    "watch-production": "NODE_ENV=production nodemon --watch server server/index.mjs --test --s3proxy --nocache --pretty",
    "styles": "NODE_ENV=development node ./scripts/build-styles.mjs",
    "styles-production": "NODE_ENV=production node ./scripts/build-styles.mjs",
    "production": "NODE_ENV=production node ./scripts/build-production.mjs",
    "post-production": "node ./scripts/post-production.mjs",
    "release": "yarn run styles-production && yarn run production && yarn run post-production",
    "deploy": "node ./scripts/deploy.mjs",
    "format": "prettier --write \"{client,server,scripts}/**/*.{ts,tsx,js,mjs,md,scss,pug}\"",
    "ncu": "yarn upgrade-interactive --latest -E",
    "ncut": "npx npm-check-updates -t greatest",
    "ncun": "npx npm-check-updates -t newest"
  }
}
