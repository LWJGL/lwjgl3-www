{
  "name": "lwjgl.org",
  "version": "1.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "The website of LWJGL",
  "author": "Apostolos Tsakpinis",
  "repository": {
    "type": "git",
    "url": "https://github.com/LWJGL/lwjgl3-www.git"
  },
  "bugs": {
    "url": "https://github.com/LWJGL/lwjgl3-www/issues"
  },
  "homepage": "https://github.com/LWJGL/lwjgl3-www#readme",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true
  },
  "browserslist": [
    "defaults",
    "chrome >= 40",
    "firefox >= 34",
    "safari >= 8",
    "edge >= 12",
    "ios >= 9",
    "ie >= 11"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "aws-sdk": "2.344.0",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "express": "5.0.0-alpha.7",
    "helmet": "3.14.0",
    "http-proxy-middleware": "0.19.0",
    "pug": "2.0.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "serve-favicon": "2.5.0",
    "yargs": "12.0.3-candidate.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.2",
    "@emotion/core": "10.0.0-beta.6",
    "@emotion/styled": "10.0.0-beta.6",
    "@reach/router": "1.3.0-alpha.1",
    "async-sema": "2.1.4",
    "autoprefixer": "9.3.1",
    "babel-loader": "8.0.4",
    "babel-plugin-emotion": "10.0.0-beta.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "benchmark": "2.1.4",
    "bootstrap": "4.1.3",
    "cache-loader": "1.2.2",
    "cli-table": "0.3.1",
    "core-js": "2.5.7",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "4.1.7",
    "exports-loader": "0.7.0",
    "fast-async": "7.0.6",
    "fbjs": "1.0.0",
    "fg-loadcss": "2.0.1",
    "fg-loadjs": "1.0.0",
    "focus-trap": "3.0.0",
    "gzip-size": "5.0.0",
    "immer": "1.7.4",
    "jsx-compress-loader": "1.1.1",
    "jszip": "3.1.5",
    "lodash-es": "4.17.11",
    "memoize-state": "2.0.4",
    "node-sass": "4.9.4",
    "postcss-loader": "3.0.0",
    "prettier": "1.14.3",
    "react": "16.7.0-alpha.0",
    "react-cache": "canary",
    "react-dom": "16.7.0-alpha.0",
    "react-fast-compare": "2.0.2",
    "react-hot-loader": "4.3.11",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "sass-loader": "7.1.0",
    "scheduler": "0.11.0-alpha.0",
    "scroll-into-view-if-needed": "2.2.20",
    "style-loader": "0.23.1",
    "terser": "3.10.4",
    "thread-loader": "1.2.0",
    "three": "0.97.0",
    "typescript-styled-plugin": "0.12.0",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "whatwg-fetch": "3.0.0"
  },
  "resolutions": {
    "upath": "1.1.0"
  },
  "scripts": {
    "start": "node server",
    "test-production": "cross-env NODE_ENV=production node server --test --s3proxy --nocache --pretty",
    "run-production": "cross-env NODE_ENV=production node server --s3proxy",
    "watch": "nodemon --watch server server/index.js",
    "watch-production": "cross-env NODE_ENV=production nodemon --watch server server/index.js --test --s3proxy --nocache --pretty",
    "styles": "node ./scripts/build-styles.js",
    "vendor": "webpack --config webpack.vendor.config.js --bail --progress",
    "bundle-analyzer-dev": "cross-env NODE_ENV=development webpack --progress --json > webpack-dev.manifest.json && webpack-bundle-analyzer webpack-dev.manifest.json",
    "bundle-analyzer-prod": "cross-env NODE_ENV=production webpack --progress --json > webpack-prod.manifest.json && webpack-bundle-analyzer webpack-prod.manifest.json",
    "bundle-verbose": "cross-env NODE_ENV=production webpack --progress --display-error-details --verbose",
    "production": "cross-env NODE_ENV=production webpack --bail --progress --json > ./public/js/webpack.manifest.json",
    "profiling": "cross-env NODE_ENV=production webpack --profiling --bail --progress --json > ./public/js/webpack.manifest.json",
    "profile": "yarn run profiling && yarn run post-production && yarn test-production",
    "post-production": "node ./scripts/post-production.js",
    "release-js": "yarn run production && yarn run post-production",
    "release": "yarn run styles && yarn run production && yarn run post-production",
    "deploy": "node ./scripts/deploy.js",
    "format": "prettier --write \"{client,server,scripts}/**/*.{js,md,scss}*\""
  }
}
