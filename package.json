{
  "name": "lwjgl.org",
  "version": "1.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "The website of LWJGL",
  "author": "Apostolos Tsakpinis",
  "repository": {
    "type": "git",
    "url": "https://github.com/LWJGL/lwjgl3.git"
  },
  "bugs": {
    "url": "https://github.com/LWJGL/lwjgl3-www/issues"
  },
  "homepage": "https://github.com/LWJGL/lwjgl3-www#readme",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true
  },
  "browserslist": [
    "chrome >= 40",
    "firefox >= 32",
    "safari >= 6",
    "edge >= 12",
    "ios >= 7",
    "ie >= 10"
  ],
  "dependencies": {
    "aws-sdk": "2.188.0",
    "body-parser": "1.18.2",
    "chalk": "2.3.0",
    "express": "5.0.0-alpha.6",
    "helmet": "3.10.0",
    "pug": "2.0.0-rc.4",
    "request": "2.83.0",
    "request-promise-native": "1.0.5",
    "serve-favicon": "2.4.5",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.38",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.38",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.38",
    "@babel/plugin-transform-react-jsx-self": "7.0.0-beta.38",
    "@babel/plugin-transform-react-jsx-source": "7.0.0-beta.38",
    "@babel/polyfill": "7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "@babel/preset-flow": "7.0.0-beta.38",
    "@babel/preset-react": "7.0.0-beta.38",
    "@babel/preset-stage-2": "7.0.0-beta.38",
    "autoprefixer": "7.2.5",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-emotion": "9.0.0-2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "benchmark": "2.1.4",
    "bootstrap": "4.0.0",
    "cache-loader": "1.2.0",
    "cli-table": "0.3.1",
    "core-js": "2.5.3",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "emotion": "9.0.0-2",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "3.0.2",
    "fast-async": "6.3.0",
    "fbjs": "0.9.0-alpha.2",
    "fg-loadcss": "2.0.1",
    "fg-loadjs": "1.0.0",
    "file-saver": "1.3.3",
    "focus-trap": "2.4.2",
    "gzip-size": "4.1.0",
    "http-proxy-middleware": "0.17.4",
    "immer": "0.8.5",
    "jszip": "3.1.5",
    "lodash-es": "4.17.4",
    "node-sass": "4.7.2",
    "nprogress": "0.2.0",
    "postcss-loader": "2.0.10",
    "prettier": "1.10.2",
    "prop-types": "15.6.0",
    "query-string": "5.0.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-emotion": "9.0.0-2",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.0.0-beta.18",
    "react-icons": "2.2.7",
    "react-loadable": "5.3.1",
    "react-perf-devtool": "3.0.4-beta1",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0-beta.1",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "three": "0.89.0",
    "typescript": "2.8.0-insiders.20180127",
    "typescript-styled-plugin": "0.4.0",
    "uglify-es": "3.3.8",
    "webpack": "4.0.0-beta.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-cli": "2.0.4",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node server",
    "test-production": "cross-env NODE_ENV=production node server --test --s3proxy --nocache --pretty",
    "run-production": "cross-env NODE_ENV=production node server --s3proxy",
    "watch": "nodemon --watch server server/index.js",
    "styles": "cross-env NODE_ENV=production webpack --config webpack.css.config.js",
    "vendor": "webpack --config webpack.vendor.config.js --bail --progress --color",
    "bundle-analyzer-dev": "cross-env NODE_ENV=development webpack --progress --json > webpack-dev.manifest.json && webpack-bundle-analyzer webpack-dev.manifest.json",
    "bundle-analyzer-prod": "cross-env NODE_ENV=production webpack --progress --json > webpack-prod.manifest.json && webpack-bundle-analyzer webpack-prod.manifest.json",
    "bundle-verbose": "cross-env NODE_ENV=production webpack --progress --color --display-error-details --verbose",
    "production": "cross-env NODE_ENV=production webpack --bail --progress --json > ./public/js/webpack.manifest.json",
    "manifest": "node ./scripts/process-manifest.js",
    "release-js": "yarn run production && yarn run manifest",
    "release": "yarn run styles && yarn run production && yarn run manifest",
    "deploy": "node ./scripts/deploy.js",
    "format": "prettier --write \"{client,server,scripts}/**/*.js*\"",
    "format-styles": "prettier --write \"client/styles/**/*.scss*\""
  }
}
