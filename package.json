{
  "name": "lwjgl.org",
  "version": "1.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "The website of LWJGL",
  "author": "Apostolos Tsakpinis",
  "repository": {
    "type": "git",
    "url": "https://github.com/LWJGL/lwjgl3-www.git"
  },
  "bugs": {
    "url": "https://github.com/LWJGL/lwjgl3-www/issues"
  },
  "homepage": "https://github.com/LWJGL/lwjgl3-www#readme",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true
  },
  "browserslist": [
    "defaults"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "aws-sdk": "2.490.0",
    "body-parser": "1.19.0",
    "chalk": "2.4.2",
    "compression": "1.7.4",
    "express": "5.0.0-alpha.7",
    "helmet": "3.18.0",
    "http-proxy-middleware": "0.19.1",
    "pug": "2.0.4",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "serve-favicon": "2.5.0",
    "yargs": "13.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@emotion/babel-preset-css-prop": "10.0.14",
    "@emotion/core": "10.0.14",
    "@reach/router": "1.3.0-alpha.1",
    "@types/jszip": "3.1.6",
    "@types/lodash-es": "4.17.3",
    "@types/reach__router": "1.2.4",
    "@types/react-dom": "16.8.4",
    "@types/scheduler": "0.12.1",
    "@types/three": "0.103.2",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "async-sema": "3.0.0",
    "autoprefixer": "9.6.1",
    "babel-loader": "8.0.6",
    "babel-plugin-optimize-react": "0.0.4",
    "babel-plugin-transform-react-pure-class-to-function": "1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "benchmark": "2.1.4",
    "bootstrap": "4.3.1",
    "cli-table": "0.3.1",
    "core-js": "3.1.4",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "cssnano": "4.1.10",
    "eslint": "6.0.1",
    "eslint-plugin-react-hooks": "1.6.1",
    "exports-loader": "0.7.0",
    "fast-async": "7.0.6",
    "fbjs": "1.0.0",
    "fg-loadcss": "2.1.0",
    "focus-trap": "5.0.1",
    "gzip-size": "5.1.1",
    "highlight.js": "9.15.8",
    "immer": "3.1.3",
    "lodash-es": "4.17.14",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "react-fast-compare": "2.0.4",
    "react-svg-loader": "3.0.3",
    "sass-loader": "7.1.0",
    "scroll-into-view-if-needed": "2.2.20",
    "style-loader": "0.23.1",
    "terser": "4.1.2",
    "three": "0.106.2",
    "typescript": "3.5.3",
    "typescript-styled-plugin": "0.14.0",
    "webpack": "5.0.0-alpha.14",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.5",
    "webpack-dev-middleware": "3.7.0",
    "webpack-hot-middleware": "2.25.0",
    "whatwg-fetch": "3.0.0"
  },
  "resolutions": {
    "upath": "1.1.2",
    "fbjs": "1.0.0"
  },
  "scripts": {
    "start": "node server",
    "lint": "eslint --ext .ts,.js,.tsx client/",
    "test-production": "cross-env NODE_ENV=production node server --test --s3proxy --nocache --pretty",
    "run-production": "cross-env NODE_ENV=production node server --s3proxy",
    "watch": "nodemon --watch server server/index.js",
    "watch-production": "cross-env NODE_ENV=production nodemon --watch server server/index.js --test --s3proxy --nocache --pretty",
    "styles": "node ./scripts/build-styles.js",
    "bundle-analyzer-dev": "cross-env NODE_ENV=development webpack --progress --json > webpack-dev.manifest.json && webpack-bundle-analyzer webpack-dev.manifest.json",
    "bundle-analyzer-prod": "cross-env NODE_ENV=production webpack --progress --json > webpack-prod.manifest.json && webpack-bundle-analyzer webpack-prod.manifest.json",
    "bundle-verbose": "cross-env NODE_ENV=production webpack --progress --display-error-details",
    "production": "cross-env NODE_ENV=production webpack --progress --display-optimization-bailout --json > ./public/js/webpack.manifest.json",
    "profiling": "yarn production --profiling",
    "profile": "yarn run profiling && yarn run post-production && yarn test-production",
    "post-production": "node ./scripts/post-production.js",
    "release-js": "yarn run production && yarn run post-production",
    "release": "yarn run styles && yarn run production && yarn run post-production",
    "deploy": "node ./scripts/deploy.js",
    "format": "prettier --write \"{client,server,scripts}/**/*.{ts,tsx,js,md,scss}*\"",
    "ncu": "yarn upgrade-interactive --latest -E",
    "ncut": "npx npm-check-updates -t",
    "ncun": "npx npm-check-updates -n"
  }
}
