  "scripts": {
    "start": "NODE_ENV=development node server",
    "debug": "NODE_ENV=development node --inspect --preserve-symlinks server",
    "lint": "eslint --ext .ts,.js,.tsx client/",
    "test-production": "NODE_ENV=production node server --test --s3proxy --nocache --pretty",
    "run-production": "NODE_ENV=production node server --s3proxy",
    "watch": "NODE_ENV=development npx nodemon --watch server server/index.js",
    "watch-production": "NODE_ENV=production nodemon --watch server server/index.js --test --s3proxy --nocache --pretty",
    "styles": "NODE_ENV=production node ./scripts/build-styles.js",
    "production": "NODE_ENV=production node ./scripts/build-production.js",
    "post-production": "node ./scripts/post-production.js",
    "release": "yarn run styles && yarn run production && yarn run post-production",
    "deploy": "node ./scripts/deploy.js",
    "format": "prettier --write \"{client,server,scripts}/**/*.{ts,tsx,js,md,scss,pug}*\"",
    "ncu": "yarn upgrade-interactive --latest -E",
    "ncut": "npx npm-check-updates -t",
    "ncun": "npx npm-check-updates -n"
  }
