{
  "name": "lwjgl.org",
  "version": "1.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "The website of LWJGL",
  "author": "Apostolos Tsakpinis",
  "repository": {
    "type": "git",
    "url": "https://github.com/LWJGL/lwjgl3.git"
  },
  "bugs": {
    "url": "https://github.com/LWJGL/lwjgl3-www/issues"
  },
  "homepage": "https://github.com/LWJGL/lwjgl3-www#readme",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true
  },
  "browserslist": [
    "chrome >= 40",
    "firefox >= 32",
    "safari >= 6",
    "edge >= 12",
    "ios >= 7",
    "ie >= 10"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "aws-sdk": "2.224.1",
    "body-parser": "1.18.2",
    "chalk": "2.3.2",
    "express": "5.0.0-alpha.6",
    "helmet": "3.12.0",
    "pug": "2.0.3",
    "request": "2.85.0",
    "request-promise-native": "1.0.5",
    "serve-favicon": "2.5.0",
    "yargs": "12.0.0-candidate.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.44",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-self": "7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-source": "7.0.0-beta.44",
    "@babel/polyfill": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-flow": "7.0.0-beta.44",
    "@babel/preset-react": "7.0.0-beta.44",
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "async-sema": "2.0.0",
    "autoprefixer": "8.2.0",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-emotion": "9.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "benchmark": "2.1.4",
    "bootstrap": "4.1.0",
    "cache-loader": "1.2.2",
    "cli-table": "0.3.1",
    "core-js": "2.5.5",
    "create-subscription": "16.3.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "cssnano": "4.0.0-rc.2",
    "emotion": "9.1.1",
    "exports-loader": "0.7.0",
    "fast-async": "7.0.0",
    "fbjs": "0.9.0-alpha.2",
    "fg-loadcss": "2.0.1",
    "fg-loadjs": "1.0.0",
    "focus-trap": "2.4.3",
    "gzip-size": "4.1.0",
    "http-proxy-middleware": "0.18.0",
    "immer": "1.2.1",
    "jszip": "3.1.5",
    "lodash-es": "4.17.8",
    "memoize-state": "1.3.3",
    "node-sass": "4.8.3",
    "nprogress": "0.2.0",
    "postcss-loader": "2.1.3",
    "prettier": "1.12.0",
    "query-string": "6.0.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-hot-loader": "4.0.1",
    "react-icons": "2.2.7",
    "react-is": "16.3.1",
    "react-loadable": "5.3.1",
    "react-perf-devtool": "3.1.3-beta.4",
    "react-router-dom": "4.3.0-rc.2",
    "redux": "4.0.0-rc.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "thread-loader": "1.1.5",
    "three": "0.91.0",
    "typescript-styled-plugin": "0.6.0",
    "uglify-es": "3.3.10",
    "webpack": "4.5.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.14",
    "webpack-dev-middleware": "3.1.2",
    "webpack-hot-middleware": "2.22.0",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "start": "node server",
    "test-production": "cross-env NODE_ENV=production node server --test --s3proxy --nocache --pretty",
    "run-production": "cross-env NODE_ENV=production node server --s3proxy",
    "watch": "nodemon --watch server server/index.js",
    "styles": "node ./scripts/build-styles.js",
    "vendor": "webpack --config webpack.vendor.config.js --bail --progress --color",
    "bundle-analyzer-dev": "cross-env NODE_ENV=development webpack --progress --json > webpack-dev.manifest.json && webpack-bundle-analyzer webpack-dev.manifest.json",
    "bundle-analyzer-prod": "cross-env NODE_ENV=production webpack --progress --json > webpack-prod.manifest.json && webpack-bundle-analyzer webpack-prod.manifest.json",
    "bundle-verbose": "cross-env NODE_ENV=production webpack --progress --color --display-error-details --verbose",
    "production": "cross-env NODE_ENV=production webpack --bail --progress --json > ./public/js/webpack.manifest.json",
    "post-production": "node ./scripts/post-production.js",
    "release-js": "yarn run production && yarn run post-production",
    "release": "yarn run styles && yarn run production && yarn run post-production",
    "deploy": "node ./scripts/deploy.js",
    "format": "prettier --write \"{client,server,scripts}/**/*.js*\"",
    "format-styles": "prettier --write \"client/styles/**/*.scss*\""
  }
}
