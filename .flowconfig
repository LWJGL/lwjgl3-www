[ignore]
<PROJECT_ROOT>/*.js
<PROJECT_ROOT>/.cache-loader/.*
<PROJECT_ROOT>/.idea/.*
<PROJECT_ROOT>/.history/.*
<PROJECT_ROOT>/.vscode/.*
<PROJECT_ROOT>/public/.*
<PROJECT_ROOT>/node_modules/@babel/.*
<PROJECT_ROOT>/node_modules/@emotion/.*
.*/fbjs/.*

[include]

[libs]

[lints]
all=warn
unclear-type=off
# untyped-type-import=error
# sketchy-null-bool=off
# sketchy-null-string=off

[options]
include_warnings=true
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
esproposal.optional_chaining=ignore
esproposal.export_star_as=enable
experimental.const_params=true
munge_underscores=true
module.system=node
module.use_strict=true
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.mjs
# Webpack alias
module.name_mapper='^~' -> '<PROJECT_ROOT>/client'
# Stub all react-icons to <Icon />
#module.name_mapper='^react-icon-base'->'<PROJECT_ROOT>/client/components/Icon.jsx'
# Stub SCSS files to avoid Flow errors
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/client/services/CSSModule.js'
